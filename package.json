{
  "name": "simple-datatables",
  "version": "1.0.0",
  "description": "Datatables for React using Material-UI 5",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --color --open",
    "prettier": "prettier --write src",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production yarn prettier && swc ./src -d ./dist && tsc",
    "build:examples": "cross-env NODE_ENV=production yarn webpack",
    "serve": "serve ./build",
    "format": "prettier --write src examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alegemaate/simple-datatables.git"
  },
  "keywords": [
    "material-ui",
    "datatables",
    "react"
  ],
  "author": "alegemaate <alegemaate@innovasium.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alegemaate/simple-datatables/issues"
  },
  "homepage": "https://github.com/alegemaate/simple-datatables#readme",
  "devDependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "@mui/styles": "^5.0.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.138",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "browserslist": "^4.19.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.3.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.2.1",
    "swc-loader": "^0.1.15",
    "typescript": "^4.4.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {},
  "side-effects": false,
  "engines": {
    "node": ">=14"
